{
    "id": "window", 
    "label": {
        "en": "Static Map Raster Calculator", 
        "es": null
    }, 
    "modelName": {
        "en": "generic_static_map", 
        "es": null
    }, 
    "targetScript": "adept.static_maps", 
    "height": 800, 
    "width": 700,
    "elements": [
        {
            "args_id": "workspace_dir", 
            "defaultValue": "~/custom_static_maps", 
            "helpText": {
